var map = new IceMap
map.init("#map-container")   

var amsrLayer = new OpenLayers.Layer.WMS("Shire", "http://shire.gina.alaska.edu", {
  layers: "amsr2_test_ice"
},{
  isBaseLayer: false,
  singleTile: true
})

trackLayer = new OpenLayers.Layer.Vector("track", {
  styleMap: new OpenLayers.StyleMap({
    pointRadius: "${total_concentration}"
    fillColor: "${color}"
  })
});

parser = new OpenLayers.Format.GeoJSON;
$.getJSON , (<%= api_cruises_path(year: @year)%>) =>
  features = parser.read(data);
  for feature in features
    feature.geometry.transform(map.getMap().displayProjection, map.getMap().getProjectionObject());

  trackLayer.addFeatures(features);

map.getMap().addLayer(amsrLayer);    
