= f.fields_for :ice_observations, ice_observation do |io|
  .col-md-4
    %h4= io.object.obs_type.capitalize
    = io.collection_select :algae_lookup_id, AlgaeLookup.all, :id, :code_with_name,
      {label: 'Algae Concentration'},{allow_blank: true, include_blank: true}
    = io.collection_select :algae_density_lookup_id, AlgaeDensityLookup.all, :id, :code_with_name,
      {label: 'Algae Density'},{allow_blank: true, include_blank: true}
    = io.collection_select :algae_location_lookup_id, AlgaeLocationLookup.all, :id, :code_with_name,
      {label: 'Algae Location'},{allow_blank: true, include_blank: true}
    = io.collection_select :sediment_lookup_id, SedimentLookup.all, :id, :code_with_name,
      {label: 'Sediment'},{allow_blank: true, include_blank: true}
